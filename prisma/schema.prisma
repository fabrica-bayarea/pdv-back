//Model das entidades

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Cliente {
  id Int @id @default(autoincrement())
  nome String
  email String @unique
  endereco String
  cpf String
  telefone String
  data_nascimento DateTime
  data_criacao DateTime @default(now())
}